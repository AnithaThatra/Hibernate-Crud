<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping package="com.hibernate.model">
   <class name = "com.hibernate.studentmain.entity.StudentEntity" table = "STUDENT">
       
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      
      <property name = "stdname" column = "name" type = "string"/>
      <property name = "stddept" column = "dept" type = "string"/>
      <property name = "stdrollno" column = "rollnumber" type = "string"/>
      
   </class>
</hibernate-mapping>